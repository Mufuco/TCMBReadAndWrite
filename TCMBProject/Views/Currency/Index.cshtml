@model List<CurrencyPostModel>
@using TCMB.EntityLayer.PostEntites
@{
    Layout = null;
}
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Code</th>
            <th scope="col">Currency Name</th>
            <th scope="col">Forex Buying</th>
            <th scope="col">Forex Selling</th>
            <th scope="col">Banknote Buying</th>
            <th scope="col">Banknote Selling</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {

            <tr>

                <td>@item.Code</td>
                <td>@item.currencyName</td>
                <td>@item.banknoteBuying</td>
                <td>@item.banknoteSelling</td>
                <td>@item.forexBuying</td>
                <td>@item.forexSelling</td>

            </tr>
        }
    </tbody>
</table>

